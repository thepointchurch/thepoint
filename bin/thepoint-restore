#!/bin/sh

python -m thepoint migrate --noinput
python -m thepoint flush --noinput
aws s3 sync "s3://${BACKUP_BUCKET}/media/" "s3://${MEDIAFILES_BUCKET}/" --quiet --delete
aws s3 cp "s3://${BACKUP_BUCKET}/data.json.bz2" - --quiet | bunzip2 >/tmp/data.json
DEBUG='' python -m thepoint loaddata /tmp/data.json
rm -f /tmp/data.json
